{
    "name": "saviobosco/UsersManager",
    "description": "UsersManager plugin for School Management System",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "UsersManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UsersManager\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
