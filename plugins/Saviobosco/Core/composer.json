{
    "name": "Saviobosco/Core",
    "description": "Saviobosco/Core plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Saviobosco\\Core\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Saviobosco\\Core\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
